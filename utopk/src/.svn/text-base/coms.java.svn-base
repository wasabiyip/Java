
public class coms {
	public static java.util.Comparator<state> getComparator(){
		return new java.util.Comparator<state>(){
			public int compare(state s1,state s2 )
			{
				if (s1.probability<s2.probability)
					return 1;
				else if(s1.probability>s2.probability)
					return -1;
				else return 0;
				
			}
		};
		
			
	}
}
